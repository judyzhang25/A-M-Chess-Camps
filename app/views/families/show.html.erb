<div class="row">
  <h4 id= "headline" class="center"><br><%= @family.family_name %><br><br></h4><br>
  <div class="col s5">
    <%= render :partial => "family_details", locals: {family: @family} %>
  </div>
  <div class="col s7">
    <%= render :partial => "partials/students_list", locals: {students: @students} unless @students.empty? %>
    <%= render :partial => "partials/registrations_list", locals: {registrations: @registrations} unless @registrations.empty? %>
  </div>
</div>
<div class="row">
  <div class="col s12">
    <% if logged_in? %>
      <%= render :partial => "partials/object_options", locals: {model_name: "family", object: @family} %>
    <% end %>
  </div>
</div>

