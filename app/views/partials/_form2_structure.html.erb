<div class="row">
  
  <div class="col s12">
    <div class="card">    
      <div class="card-content">
        

        <span class="card-title"><h4 id= "headline" class="center"><br><%= type_of_form %><br><br></h4><br></span>
        <%= simple_form_for(object) do |f| %>
          <%= f.error_notification %>
          <div class="row">
            <div class="form-inputs">
              <%= render partial: "#{fields1}", locals: { f: f } %>
            </div>
          </div>
      </div>
    </div>

    <div class="row">
      <div class="col s12">
        <div class="card grey lighten-3">
          <div class="row">
            <div class="card-content">
              <span class="card-title"><h4 id= "headline" class="center"><br><%= section_2 %><br><br></h4><br></span>
              <div class="form-inputs">
                <%= render partial: "#{fields2}", locals: { f: f } %>
              </div>
            </div>
          </div>    
        </div>              
      </div>
    </div>

      <div class="row">
        <div class="form-actions col s9">
          <%= f.button :submit %>
        </div>
      </div>
    <% end %>
  </div>
</div>
<%= link_to 'Back', :back %>